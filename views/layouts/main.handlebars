  {{> helperscripts }}

</head>

<body>

  {{> navbar }}

  <main id="bodyContainer" class="container" style="overflow:hidden;">

    {{{body}}}

  </main>

  <div class="modal fade" id="boostersModal" tabindex="-1" role="dialog" aria-labelledby="boostersModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="boostersModalLabel">Boosted by</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <ul class="list-group list-group-flush" id="boosters-list"></ul>
      </div>
    </div>
  </div>

</body>

{{> scripts}}

{{> uploadscripts}}

</html>

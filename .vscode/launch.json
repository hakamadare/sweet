    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Create 'boostsV2' field",
            "program": "${workspaceFolder}\\databasetransforms\\populateBoostsProperly.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "nodemon Launch Server",
            "runtimeExecutable": "nodemon",
            "program": "${workspaceFolder}/server.js",
            "restart": true,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Server",
            "program": "${workspaceFolder}\\server.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Add fake users",
